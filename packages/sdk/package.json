{
  "name": "@unfrl/usufruct-sdk",
  "description": "API Documentation",
  "version": "1.0.0",
  "dependencies": {
    "@azure/core-http": "^1.1.4",
    "tslib": "^1.9.3"
  },
  "main": "./dist/Usufruct.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "devDependencies": {
    "typescript": "^3.1.1",
    "rollup": "^0.66.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "uglify-js": "^3.4.9",
    "@microsoft/api-extractor": "7.9.10"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "esm/**/*.js",
    "esm/**/*.js.map",
    "esm/**/*.d.ts",
    "esm/**/*.d.ts.map",
    "src/**/*.ts",
    "README.md",
    "rollup.config.js",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc && rollup -c rollup.config.js && npm run minify && npm run extract-api",
    "minify": "uglifyjs -c -m --comments --source-map \"content='./dist/Usufruct.js.map'\" -o ./dist/Usufruct.min.js ./dist/Usufruct.js",
    "prepack": "npm install && npm run build",
    "extract-api": "api-extractor run --local",
    "watch": "tsc -w"
  }
}